# Backend Environment Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DB_HOST=your-rds-endpoint.region.rds.amazonaws.com
DB_PORT=5432
DB_NAME=agentdb
DB_USER=your-db-username
DB_PASSWORD=your-secure-password
DB_SSL=true

# JWT Configuration
JWT_SECRET=your-jwt-secret-change-in-production
JWT_EXPIRY=7d

# CORS Configuration
CORS_ORIGIN=https://your-domain.com,http://localhost:5173

# AWS Configuration
AWS_REGION=us-east-1

# AgentCore Agent ARNs
SECOPS_AGENT_ARN=arn:aws:bedrock-agentcore:region:account-id:runtime/secops_agent-xxxxx
SECURITY_AGENT_ARN=arn:aws:bedrock-agentcore:region:account-id:runtime/security_agent-xxxxx
TRIAGE_AGENT_ARN=arn:aws:bedrock-agentcore:region:account-id:runtime/triage_agent-xxxxx
MONITORING_AGENT_ARN=arn:aws:bedrock-agentcore:region:account-id:runtime/monitoring_agent-xxxxx
BULK_ANALYSIS_AGENT_ARN=arn:aws:bedrock-agentcore:region:account-id:runtime/bulk_analysis_agent-xxxxx
BACKEND_API_URL=https://your-domain.com/api

# SNS Configuration
SNS_TOPIC_ARN_CRITICAL=arn:aws:sns:region:account-id:soc-lite-critical-alerts
SNS_TOPIC_ARN_MONITORING=arn:aws:sns:region:account-id:soc-lite-monitoring-alerts
ALERT_EMAIL=your-email@example.com
